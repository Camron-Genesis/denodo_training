CREATE OR REPLACE VIEW iv_hr_denormalized FOLDER = '/2 - integration' AS SELECT bv_hr_regions.region_name AS region_name, bv_hr_countries.country_name AS country_name, bv_hr_locations.location_id AS location_id, bv_hr_locations.city AS city, bv_hr_departments.department_id AS department_id, bv_hr_departments.department_name AS department_name, bv_hr_employees.first_name AS first_name, bv_hr_employees.last_name AS last_name, bv_hr_employees.salary AS salary, bv_hr_employees.commission_pct AS commission_pct, bv_hr_employees.manager_id AS manager_id, bv_hr_jobs.job_title AS job_title, bv_hr_jobs.min_salary AS min_salary, bv_hr_jobs.max_salary AS max_salary FROM ((((bv_hr_regions AS bv_hr_regions FULL OUTER JOIN bv_hr_countries AS bv_hr_countries ON bv_hr_regions.region_id = bv_hr_countries.region_id ) FULL OUTER JOIN bv_hr_locations AS bv_hr_locations ON bv_hr_countries.country_id = bv_hr_locations.country_id ) FULL OUTER JOIN bv_hr_departments AS bv_hr_departments ON bv_hr_locations.location_id = bv_hr_departments.location_id ) FULL OUTER JOIN bv_hr_employees AS bv_hr_employees ON (bv_hr_departments.department_id = bv_hr_employees.department_id AND bv_hr_departments.manager_id = bv_hr_employees.employee_id) ) FULL OUTER JOIN bv_hr_jobs AS bv_hr_jobs ON bv_hr_employees.job_id = bv_hr_jobs.job_id ;

ALTER VIEW iv_hr_denormalized
 LAYOUT (bv_hr_regions = [20, 20, 200, 65], bv_hr_countries = [20, 275, 200, 85], bv_hr_locations = [20, 390, 200, 145], bv_hr_departments = [20, 565, 200, 105], bv_hr_employees = [20, 700, 200, 225], bv_hr_jobs = [20, 115, 200, 105]);

