CREATE OR REPLACE VIEW iv_product_details_array FOLDER = '/2 - integration/product' AS SELECT (bv_product_details.return).product.code AS code, (bv_product_details.return).product.description AS description, (bv_product_details.return).product.warnings AS warnings, (bv_product_details.return).product.numresults AS numresults, (bv_product_details.return).product.productdetails AS productdetails FROM bv_product_details;

